    <string name="app_name">KeySafe</string>
    <string name="why_is_master_password_important">Master lozinka za aplikaciju za čuvanje lozinki je kao siguran ključ koji štiti pristup svim vašim dragocenim ključevima, osiguravajući da samo vi možete otključati i koristiti ih.</string>
